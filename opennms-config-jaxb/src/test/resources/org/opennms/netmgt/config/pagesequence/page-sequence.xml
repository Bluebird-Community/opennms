<page-sequence>
	<page method="GET" http-version="1.1" scheme="http" host="${ipaddr}"
		disable-ssl-verification="true" port="8980" path="/opennms/login.jsp"
		successMatch="Password" response-range="100-399" />
	<page method="POST" http-version="1.1" scheme="http" host="${ipaddr}"
		disable-ssl-verification="true" port="8980" path="/opennms/j_spring_security_check"
		failureMatch="(?s)(The username or password provided does not match our records)|(You are not signed in)"
		failureMessage="Login in Failed" successMatch="Log Out"
		response-range="100-399">
		<parameter key="j_username" value="admin" />
		<parameter key="j_password" value="admin" />
	</page>
	<page method="GET" http-version="1.1" scheme="http" host="${ipaddr}"
		disable-ssl-verification="true" port="8980" path="/opennms/j_spring_security_logout"
		successMatch="Login" response-range="100-399" >
		<header name="foo" value="bar"/>
	</page>
</page-sequence>
