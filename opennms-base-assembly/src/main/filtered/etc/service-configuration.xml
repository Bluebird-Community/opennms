  <service enabled="true"><name>OpenNMS:Name=Queued</name></service>
  <service enabled="true"><name>OpenNMS:Name=Actiond</name></service>
  <service enabled="true"><name>OpenNMS:Name=Notifd</name></service>
  <service enabled="true"><name>OpenNMS:Name=Scriptd</name></service>
  <service enabled="true"><name>OpenNMS:Name=Rtcd</name></service>
  <service enabled="true"><name>OpenNMS:Name=Pollerd</name></service>
  <service enabled="false"><name>OpenNMS:Name=SnmpPoller</name></service>
  <service enabled="true"><name>OpenNMS:Name=EnhancedLinkd</name></service>
