  <service enabled="true"><name>OpenNMS:Name=KarafStartupMonitor</name></service>
  <service enabled="false"><name>OpenNMS:Name=Syslogd</name></service>
  <service enabled="true"><name>OpenNMS:Name=Telemetryd</name></service>
  <service enabled="true"><name>OpenNMS:Name=Trapd</name></service>
